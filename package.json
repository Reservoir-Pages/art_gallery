  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "gulp",
    "dev": "gulp",
    "dev:pug": "gulp --pug",
    "build": "cross-env NODE_ENV=prod gulp build && html-validator build",
    "build:full": "cross-env NODE_ENV=prod gulp build --full && html-validator build --ignore=Trailing slash on void elements",
    "deploy": "cross-env NODE_ENV=prod gulp deploy",
    "deploy:prod": "cross-env NODE_ENV=prod gulp deploy",
    "zip": "cross-env NODE_ENV=prod gulp build --full && html-validator build --ignore=Trailing slash on void elements && gulp zip",
    "v3c": "html-validator build --continue"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
